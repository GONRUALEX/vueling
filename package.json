{
  "name": "airports-of-the-world",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all:windows": "start cmd.exe /k \"color 0F && ng serve\" & start cmd.exe /k \"color 0F && stubby -s 1500 -l 127.0.0.1 -d stubbs/stubby.json -w\"",
    "start:all:linux": "gnome-terminal -- bash -c \"echo -e '\\033[42;97m' && ng serve; exec bash\" & gnome-terminal -- bash -c \"echo -e '\\033[42;97m' && stubby -s 1500 -l 127.0.0.1 -d stubbs/stubby.json -w; exec bash\"",
    "ng:serve": "node_modules/.bin/ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stubs": "stubby -s 1500 -l 127.0.0.1 -d stubbs/stubby.json -w"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/forms": "^13.0.0",
    "@angular/localize": "^13.0.0",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.0",
    "@angular/cli": "^13.0.0",
    "@angular/compiler-cli": "^13.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "^7.0.0",
